#!/usr/local/bin/perl

use MSPDI;
use Inclusion;

sub differentialInclusion { 
  my ($x,$y) = @_; 
  my (($x_left, $y_left) ,($x_right, $y_right));

  $x_left = ($x + $y)*0.97 - $x;
  $y_left = ($y - $x)*0.97 - $y;

  $x_right = ($x + $y)*0.99 - $x;
  $y_right = ($y - $x)*0.99 - $y;

  return(($x_left, $y_left) ,($x_right, $y_right) ); 
}

halfrectanglesCentre ((-100,-100), (100,100), (15, 15));
